<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <button class="close" type='button' id="add_close" data-dismiss='modal' aria-hidden='true'></button>
            <h4 class="modal-title" id="ajaxModal"> 添加字典配置 </h4>
        </div>
        <div class="modal-body">
            <div class="panel panel-default">
                <div class="panel-heading">
                </div>
                <div class="panel-body">
                    <form action="" class="form-horizontal">
                        <div class="control-group">
                            <label class="control-label">字典编码</label>
                            <div class="controls">
                                <input type="text" name="code" class="m-wrap" />
                                <span class="help-inline"> <i class="icon-exclamation-sign text-info"></i> 编码必须唯一</span>
                            </div>
                        </div>
                        <div class="control-group">
                            <label class="control-label">字典名称</label>
                            <div class="controls">
                                <input type="text" name="dictName" class="m-wrap" />
                            </div>
                        </div>
                        <div class="control-group">
                            <label class="control-label">字典值</label>
                            <div class="controls">
                                <input type="text" name="dictValue" class="m-wrap" />
                            </div>
                        </div>
                        <div class="control-group">
                            <label class="control-label">排序</label>
                            <div class="controls">
                                <input type="number" name="sort" class="m-wrap" value="0"/>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button class="btn btn-default" type='button'  onclick="add_save()">
                <span class="glyphicon glyphicon-remove">保存</span>
            </button>
            <button class="btn btn-default" type='button' data-dismiss='modal'>
                <span class="glyphicon glyphicon-remove">取消</span>
            </button>
        </div>
    </div>
</div>